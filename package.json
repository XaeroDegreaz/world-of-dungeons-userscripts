{
  "name": "world-of-dungeons-userscripts",
  "description": "Collection of userscripts for World of Dungeons",
  "version": "1.0.0",
  "author": {
    "name": "XaeroDegreaz"
  },
  "userScripts": {
    "display-skill-rolls": {
      "version": "2021.06.28.0"
    },
    "script2": {
      "version": "2021.06.28.0"
    }
  },
  "eslintIgnore": [
    "dist/**/*.js",
    "release/*.js",
    "node_modules"
  ],
  "scripts": {
    "lint": "eslint --ext .ts,.js src",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags",
    "analize": "npm_config_report=true npm run build",
    "build": "webpack --mode production --config config/webpack.config.production.cjs",
    "dev": "webpack --mode development --config config/webpack.config.dev.cjs",
    "test": "mocha -r ts-node/register -r @babel/register 'test/**/*.spec.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XaeroDegreaz/world-of-dungeons-userscripts"
  },
  "private": true,
  "dependencies": {
    "axios": "0.21.1",
    "axios-userscript-adapter": "0.1.4",
    "rpg-dice-roller": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.5",
    "@babel/register": "^7.14.5",
    "@types/greasemonkey": "^4.0.1",
    "@types/jquery": "^3.5.5",
    "@types/jsdom": "^16.2.12",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "babel-loader": "8.2.2",
    "browserslist": "4.16.6",
    "css-loader": "5.2.6",
    "expect": "^27.0.2",
    "jquery": "^3.6.0",
    "jsdom": "^16.6.0",
    "less": "4.1.1",
    "less-loader": "10.0.0",
    "mocha": "^9.0.1",
    "prettier": "^2.3.2",
    "style-loader": "2.0.0",
    "ts-loader": "9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "4.3.4",
    "userscript-metadata-webpack-plugin": "0.1.0",
    "webpack": "5.39.1",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-livereload-plugin": "3.0.1",
    "webpack-merge": "5.8.0"
  }
}
